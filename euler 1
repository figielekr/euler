public class euler {


    public static void main(String[] args) {

         //euler result = new euler();
        System.out.println(divisors(3,5));
        
    }
    public static int divisors(int a, int b) {
        int sum=0;
        for (int i = 0; i <= 1000; i++) {
            if (i % a == 0 || i % b == 0) {
                sum += i;
            }
        }
        return sum;
        //System.out.println(sum);
    }
}


