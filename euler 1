public class euler {
    public static void main(String[] args) {
        int[] tab = new int[1000];
        for (int i = 0; i<=tab.length-1; i++){
            if (i%3==0 || i%5==0){
                tab[i]=i;
            }
            /*else{
                tab[i]=0;
            }*/
        }
        int sum=0;
        for (int i = 0; i<=tab.length-1; i++){
            sum+=tab[i];
        }
        System.out.println(sum);
    }
}
